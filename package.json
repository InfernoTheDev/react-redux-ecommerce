{
  "name": "trump-is-a-baby",
  "version": "1.0.0",
  "description": "website selling baby trump gifts",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd client && npm start",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "server": "nodemon server.js --ignore client"
  },
  "keywords": [
    "trump"
  ],
  "author": "Justin Kunz",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.10",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.19.4",
    "path": "^0.12.7",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "stripe": "^7.13.0"
  }
}
